{
  "name": "ksoft/klaravel",
  "description": "Laravel control panel for developers, comes with integrated Scaffold generator, backups, logs, and built in instructions.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kiko Seijo",
      "email": "kiko@sunnyface.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ksoft\\Klaravel\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "anahkiasen/former": "^5.1",
    "spatie/laravel-activitylog": "^4.0",
    "spatie/laravel-backup": "^8.0"
  },
  "require-dev": {
    "laravel/laravel": "10.0.*",
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ksoft\\Klaravel\\ServiceProvider"
      ],
      "aliases": {
        "Larapp": "Ksoft\\Klaravel\\Facade"
      }
    }
  }
}
